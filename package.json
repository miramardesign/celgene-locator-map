{
  "name": "celgene-locator-map",
  "version": "0.0.34",
  "main": "electron.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "ng build --base-href ./ && electron .",
    "build-windows": "ng build --base-href ./  --prod && electron-packager . --overwrite --asar --prune=true --ignore=README.md --platform=win32 --out=electron-builds",
    "build-mac": "ng build --base-href ./  --prod && electron-packager . --overwrite --asar --ignore=README.md --platform=darwin --out=electron-builds",
    "deploy": "ng build --prod --base-href \"https://miramardesign.github.io/celgene-locator-map/celgene-locator-map/\"  && ngh",
    "patch": "  git add package.json && npm version patch",
    "zip": "bestzip ../%npm_package_name%-%npm_package_version%.zip ../%npm_package_name%/src/* ../%npm_package_name%/*.json ../%npm_package_name%/*.js  ../%npm_package_name%/*.md  ../%npm_package_name%/*.gitignore && git add package.json && npm run up",
    "zipbuild": "bestzip ../%npm_package_name%-%npm_package_version%-exe-win32-x64.zip ../%npm_package_name%/electron-builds/%npm_package_name%-win32-x64/*  ",
    "up": "gdrive-windows-x64 upload ../%npm_package_name%-%npm_package_version%.zip",
    "upbuild": "gdrive-windows-x64 upload ../%npm_package_name%-%npm_package_version%-exe-win32-x64.zip"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.7",
    "@angular/common": "~8.2.7",
    "@angular/compiler": "~8.2.7",
    "@angular/core": "~8.2.7",
    "@angular/forms": "~8.2.7",
    "@angular/platform-browser": "~8.2.7",
    "@angular/platform-browser-dynamic": "~8.2.7",
    "@angular/router": "~8.2.7",
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.1",
    "popper": "^1.0.1",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.5",
    "@angular/cli": "~8.3.5",
    "@angular/compiler-cli": "~8.2.7",
    "@angular/language-service": "~8.2.7",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "asar": "^2.0.1",
    "codelyzer": "^5.0.0",
    "electron": "^6.0.10",
    "electron-packager": "^14.0.6",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jszip-cli": "^1.4.24",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "angular-cli-ghpages": "^0.6.0",
    "adm-zip-cli": "^1.0.0"
  }
}
